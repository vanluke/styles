{
  "name": "lg-styled-component",
  "version": "0.0.1",
  "description": "Styled component",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "npm run clean:server && node_modules/.bin/tsc --p tsconfig.server.json && npm run copy:keys",
    "use:node": "nvm use v7.8.0",
    "start:server": "node ./build",
    "clean:server": "node_modules/.bin/rimraf build/",
    "copy:keys": "node_modules/.bin/copyfiles -u 1 server/keys/* build/ && node_modules/.bin/copyfiles -u 1 server/authentication/secret/* build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanluke/the-app-with-a-style.git"
  },
  "keywords": [
    "React",
    "Styled",
    "Components",
    "Redux",
    "Observable"
  ],
  "author": "Lukasz Gonciarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanluke/the-app-with-a-style/issues"
  },
  "homepage": "https://github.com/vanluke/the-app-with-a-style#readme",
  "devDependencies": {
    "@types/co-body": "0.0.1",
    "@types/convict": "^4.0.0",
    "@types/jest": "^20.0.2",
    "@types/jsonwebtoken": "^7.2.2",
    "@types/koa": "^2.0.39",
    "@types/koa-json": "^2.0.16",
    "@types/koa-mount": "^2.0.18",
    "@types/koa-router": "^7.0.22",
    "@types/mongodb": "^2.2.7",
    "@types/node": "^8.0.10",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
